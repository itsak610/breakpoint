            }
            .hamburger-menu .menu-item {
                display: block;
                height: 2px;
                margin: 0 0 7px;
                transition: all 0.3s ease-in-out;
                background-image: linear-gradient(to bottom right, #42E6FF, #F11D7A);
                background-size: 100px;
                width: 30px;
            }
            .hamburger-menu.open .menu-item {
                margin: 0 0 5px;
