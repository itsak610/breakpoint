        script.
            document.getElementById("homenav").classList.toggle("active");
            const mediaQuery = window.matchMedia('(max-width: 750px)')
            if (mediaQuery.matches) {
                var size = { SPHERE_SIZE: 120 };

                var scene, camera, renderer;
        
                var time = 0;
                var mouseX = 0;
                var mouseY = -1000;

                var vertexShader = document.getElementById("vertexShader").textContent;
                var fragmentShader = document.getElementById("fragmentShader").textContent;

                var scene = new THREE.Scene();

                const canvas = document.querySelector("canvas.webgl");

                var camera = new THREE.OrthographicCamera(
                    window.innerWidth * -0.5,
                    window.innerWidth * 0.5,
                    window.innerHeight * 0.5,
                    window.innerHeight * -0.5,
                    -1000,
                    1000
                );
                camera.position.z = 500;
                scene.add(camera);

                var geometry = new THREE.IcosahedronGeometry(size.SPHERE_SIZE, 5);

                var material;

                var uniforms = {
                    time: {
                        type: "f",
                        value: 1.0
                    },
                    mouse: {
                        type: "v2",
                        value: new THREE.Vector2(mouseX, -mouseY)
                    },
                    size: {
                        type: "v2",
                        value: new THREE.Vector2(100, 100)
                    },
                };

                var material = new THREE.ShaderMaterial({
                    uniforms: uniforms,
                    vertexShader: vertexShader,
                    fragmentShader: fragmentShader,
                    wireframe: true,
