        style.
            @import url('https://fonts.googleapis.com/css2?family=Chakra+Petch&display=swap');
            * {
                margin: 0;
                padding: 0;
            }
            html:focus-within {
                scroll-behavior: smooth;
                background: black;
            }

            body {
                font-family: 'Chakra Petch', sans-serif;
                background: linear-gradient(rgba(0, 0, 0, .5), rgba(0, 0, 0, .5)), url(https://distracted-wescoff-b1d8d6.netlify.app/img/background.jpg);
