            varying float intensity;

            void main() {
                // float greyvalue = (vVertexColor.x + vVertexColor.y + vVertexColor.z) / 3.0 * intensity * .5;
                gl_FragColor = vec4(intensity * 2., intensity * 0.3, intensity, 1.0);
