            .hamburger-menu .menu-item:nth-child(2), .hamburger-menu .menu-item:nth-child(3) {
                background-position-y: 50px;
            
            }
            .hamburger-menu.open .menu-item:first-child {
            
                transform: rotate(45deg);
                transform-origin: 6px;
